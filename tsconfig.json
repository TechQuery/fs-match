{
    "compilerOptions": {
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "jsx": "react",
        "jsxFactory": "createCommand",
        "lib": ["ES2019"],
        "target": "ES2017",
        "declaration": true,
        "outDir": "dist/"
    },
    "include": ["source/*"],
    "typedocOptions": {
        "name": "FS Match",
        "mode": "file",
        "excludeExternals": true,
        "excludeNotExported": true,
        "readme": "./ReadMe.md",
        "out": "docs/"
    }
}
